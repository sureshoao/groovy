import groovy.time.*
public class hello {
  // testing 2 different ways to fizzbuzz
    static int start = 1
  static int end = 100
  static int fizz = 3
  static int buzz = 5

  static def main(args) {
    
    def timeStart = new Date()
    (start..end).each{println "${it%fizz?'':'Fizz'}${it%buzz?'':'Buzz'}" ?: it }
    def timeStop = new Date()
    TimeDuration duration = TimeCategory.minus(timeStop, timeStart)
    println(duration)
    1.upto(3, {println("")})
    timeStart = new Date()
    for (int i = start; i <= end; i++) {
      if (i % fizz == 0) print("Fizz")
      if (i % buzz == 0) print("Buzz")
      if (i % fizz != 0 && i % buzz != 0) print(i)
      print("\n")
    }
    timeStop = new Date()
    duration = TimeCategory.minus(timeStop, timeStart)
    println(duration)
  }
